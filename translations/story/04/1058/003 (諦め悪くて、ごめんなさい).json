                },
                {
                    "origLen": 26,
                    "pathId": -281512324191867260
                }
            ],
            "pathId": 2863399315924415138,
            "blockIdx": 36
        },
        {
            "jpName": "メイショウドトウ",
            "enName": "",
            "jpText": "…………。\r\n……えへへ。うれしいです……。",
            "enText": "",
            "nextBlock": 38,
            "origClipLength": 238,
            "voiceIdx": 27,
            "animData": [
                {
                    "origLen": 117,
                    "pathId": -5776936584204181709
