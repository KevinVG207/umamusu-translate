            "animData": [
                {
                    "origLen": 325,
                    "pathId": 3623540153500683397
                },
                {
                    "origLen": 256,
                    "pathId": -2869561366359735029
                }
            ],
            "pathId": 4588906703584874272,
            "blockIdx": 35
        },
        {
            "jpName": "メイショウドトウ",
            "enName": "",
            "jpText": "そう、なん、ですか……。",
            "enText": "",
            "nextBlock": 37,
            "origClipLength": 141,
            "voiceIdx": 26,
            "animData": [
