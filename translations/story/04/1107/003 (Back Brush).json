            "animData": [
                {
                    "origLen": 212,
                    "pathId": 4852208329374836586
                },
                {
                    "origLen": 129,
                    "pathId": 7406965148735938506
                }
            ],
            "pathId": 7792624539686699379,
            "blockIdx": 15
        },
        {
            "jpName": "タップダンスシチー",
            "enName": "Tap Dance City",
            "jpText": "へえ……1ハロン12.4秒のペースで走れ、と。\nまた随分と細かい指示だこと。",
            "enText": "",
            "nextBlock": 17,
            "origClipLength": 302,
            "voiceIdx": 11,
            "animData": [
                {
                    "origLen": 142,
                    "pathId": -5702727892384624719
