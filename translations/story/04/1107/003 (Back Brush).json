            "animData": [
                {
                    "origLen": 212,
                    "pathId": 7602562164503740722
                },
                {
                    "origLen": 66,
                    "pathId": 2146161639850353597
                }
            ],
            "pathId": 1654627745982001122,
            "blockIdx": 14
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "集めたデータでは、この中には積極的に前へ出る娘はいないはず。\n恐らくスローペースの展開になるだろう。",
            "enText": "",
            "nextBlock": 16,
            "origClipLength": 110,
            "voiceIdx": -1,
            "choices": [
                {
                    "jpText": "だから休憩……息を入れる隙を与えない",
                    "enText": "",
                    "nextBlock": 16
