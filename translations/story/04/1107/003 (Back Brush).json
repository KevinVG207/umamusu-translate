            "pathId": -1015508528852590082,
            "blockIdx": 12
        },
        {
            "jpName": "タップの友人たち",
            "enName": "",
            "jpText": "いえ～～い！",
            "enText": "",
            "nextBlock": 14,
            "origClipLength": 56,
            "voiceIdx": 9,
            "animData": [
                {
                    "origLen": 70,
                    "pathId": 9152209539852239653
