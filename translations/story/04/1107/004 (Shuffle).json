            "pathId": -3817899440135131835,
            "blockIdx": 27
        },
        {
            "jpName": "ツインテの女子高校生",
            "enName": "",
            "jpText": "……！",
            "enText": "",
            "nextBlock": 29,
            "origClipLength": 33,
            "voiceIdx": 27,
            "animData": [
                {
                    "origLen": 214,
                    "pathId": -8864444914209973087
                }
