            "pathId": -4018887016344105595,
            "blockIdx": 7
        },
        {
            "jpName": "ネオユニヴァース",
            "enName": "",
            "jpText": "…………“交信”。",
            "enText": "",
            "nextBlock": 9,
            "origClipLength": 155,
            "voiceIdx": 4,
            "animData": [
                {
                    "origLen": 33,
                    "pathId": -3567186934364848412
