            "pathId": -4087568126510184031,
            "blockIdx": 12
        },
        {
            "jpName": "ネオユニヴァース",
            "enName": "",
            "jpText": "…………。",
            "enText": "",
            "nextBlock": 14,
            "origClipLength": 79,
            "voiceIdx": 6,
            "animData": [
                {
                    "origLen": 89,
                    "pathId": -8022648541003064921
